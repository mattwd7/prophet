.column#left
  .feedback-summary.filters
    #h2_Resonance Resonance
    %ul
      %li
        .description.resonant Resonant
        .number-bubble#resonant= @my_feedbacks.resonant.count.count
      %li
        .description.mixed Mixed
        .number-bubble#mixed= @my_feedbacks.mixed.count.count
      %li
        .description.isolated Isolated
        .number-bubble#isolated= @my_feedbacks.isolated.count.count
  = render partial: 'users/profile'



.column#middle
  #filter-tags
    .resonance-tags

  = render partial: 'feedbacks/form'

  #feedbacks
    = render partial: 'feedbacks/index', locals: { my_feedbacks: @my_feedbacks, team_feedbacks: @team_feedbacks }



.column#right


%table#admin-grid
  %tbody